<h1><%= @list.name %></h1>
<%= link_to 'Add Bookmark', new_list_bookmark_path(@list) %>
<h2>Bookmarks</h2>
<ul>
  <% @list.bookmarks.each do |bookmark| %>
    <li>
      <%= bookmark.movie.title %> - <%= bookmark.comment %>
      <%= link_to 'Delete', bookmark_path(bookmark), method: :delete %>
    </li>
  <% end %>
</ul>
